{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "define": true,
        "requirejs": true,
        "m": true,
        "Twirlip7": true,
        "open": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-console":  "off",
        "no-unused-vars":  "off",
        "no-case-declarations": "off",
        "no-inner-declarations": "off"
    }
}
